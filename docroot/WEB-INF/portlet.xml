		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<resource-bundle>content/Language</resource-bundle>
		<portlet-info>
			<title>carousel</title>
			<short-title>carousel</short-title>
			<keywords>carousel</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
